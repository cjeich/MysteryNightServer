  <li>
    <strong>Location:</strong>
    <%= @station.location_id %>
  </li>

  <li>
    <strong>Open:</strong>
    <%= @station.open %>
  </li>

  <li>
    <strong>Notes:</strong>
    <%= @station.notes %>
