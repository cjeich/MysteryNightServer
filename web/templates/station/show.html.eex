<h2>Show station</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @station.name %>
  </li>

  <li>
    <strong>Address1:</strong>
    <%= @station.address1 %>
  </li>

  <li>
    <strong>Address2:</strong>
    <%= @station.address2 %>
  </li>

  <li>
    <strong>City:</strong>
