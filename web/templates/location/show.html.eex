<h2>Show location</h2>

<ul>

  <li>
    <strong>Lat:</strong>
    <%= @location.lat %>
  </li>

  <li>
    <strong>Long:</strong>
    <%= @location.long %>
  </li>

  <li>
    <strong>Velocity:</strong>
    <%= @location.velocity %>
  </li>

  <li>
    <strong>Station:</strong>
    <%= @location.station_id %>
  </li>

</ul>

<%= link "Edit", to: location_path(@conn, :edit, @location) %>
<%= link "Back", to: location_path(@conn, :index) %>
